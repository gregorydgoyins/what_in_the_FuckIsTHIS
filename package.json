    "knip": "^3.13.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2",
    "vite": "^5.4.19",
    "vite-bundle-analyzer": "^0.8.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
